@base <http://example.com/LL/comuammand_na_n-genelach.ttl>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rel: <http://purl.org/vocab/relationship/>.
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix irishRel: <http://example.com/earlyIrishRelationship.ttl#> .

<>
    a dctype:Dataset;
    dcterms:title "Comuammand na n-Genelach"@sga;
    dcterms:isFormatOf <http://www.ucc.ie/celt/published/G800011F/text004.html>;
    dcterms:format "text/turtle".

<#Erímon>
    a foaf:Person;
    irishRel:nomName "Erímon";
    rdfs:comment "Ic h-Erímon tra condrecat na teora Connachta. & h. Neill in tuascirt & h. Neill in descirt & Airgialla & na Dési & Laigin & Ossairge. & Éraind. & Orbraigi. & Fotharta. & Dal Riata. & Dal Fiatach. Et rigrad Alban. Et Síl Conaire & Cland Oengusa & Fergusa & Loairn tri mc h-Eirc. Et Síl Conaire i n-Albain & i nh-Erinn .i. Muscraigi & Corco Duibni.";
    owl:sameAs <http://example.com/LL/clanna_ébir_i_l-leith_chuind.ttl#Erimon>;
    owl:sameAs <http://example.com/LL/lagin.ttl#h-Erimoin>.

<#Éber>
    a foaf:Person;
    irishRel:nomName "Éber";
    rdfs:comment "Ic h-Éber condrecat na secht n-Eoganachta & Lemnaig Alban.";
    owl:sameAs <http://example.com/LL/eoganachta_casil.ttl#Ebir>.

<#Ír>
    a foaf:Person;
    irishRel:nomName "Ír";
    rdfs:comment "Ic h-Ír condrecat Síl Fergusa & Conaill & Celtchair.";
    owl:sameAs <http://example.com/LL/clanna_ébir_i_l-leith_chuind.ttl#Ír>.

<#OengusTurbech>
    a foaf:Person;
    irishRel:nomName "Oengus Turbech";
    rdfs:comment "Ic Oengus Turbech condrecat Ulaid & Albanaig & Ernai frisna clanna remaind.";
    owl:sameAs <http://example.com/LL/clanna_ébir_i_l-leith_chuind.ttl#OengusaTurbig>.

<#Niall>
    a foaf:Person;
    irishRel:nomName "Niall";
    rdfs:comment "Ic Niall condrecat .h. Néill in descirt & in tuascirt. & uii. fini Temrach .i. Conall. Colman. Eogan. Aed Slane. Tethba & Cairpre. & Cenel n-Ennae & Loegaire.";
    owl:sameAs <http://example.com/LL/fer_tethba.ttl#NeilNoigiallaig>.

<#EochaidMugmedón>
    a foaf:Person;
    irishRel:nomName "Eochaid Mugmedón";
    owl:sameAs <http://example.com/LL/rig_ailig.ttl#EchachMugmedóin>;
    rdfs:comment "Ic Eochaid Mugmedón condrecat .h. Neill & Connachta.".

<#CarpriuLiphechair>
    a foaf:Person;
    irishRel:genName "Carpriu Liphecahir";
    irishRel:nomName "Coirpre Liphechair";
    owl:sameAs <http://example.com/LL/rig_ailig.ttl#CarpriLiphechair>;
    rdfs:comment "Ic Carpriu Liphechair condrecat Airgialla & h. Meic Cuais fri Ú Néill & Connachta & Corco Roidi fri Claind Cuind.".

<#AedFindlíath>
    a foaf:Person;
    irishRel:nomName "Aed Findlíath";
    owl:sameAs <http://example.com/LL/rig_ailig.ttl#AedaFindleith>;
    rdfs:comment "Ic Aed Findlíath condrecat Cland Néill & Cland Domnaill.".

<#FeidlimidRechtaid>
    a foaf:Person;
    irishRel:nomName "Feidlimid Rechtaid";
    owl:sameAs <http://example.com/LL/lagin.ttl#FeidlimidRechtada>;
    rdfs:comment "Ic Feidlimid Rechtaid condrecat Fotharta & na Dési fri Claind Cuin.".

<#UgaineMár>
    a foaf:Person;
    irishRel:nomName "Ugaine Már";
    rdfs:comment "Ic Ugaine Már condrecat Lagin & Ossairge & Cland Chuind".

<#BresulBricc>
    a foaf:Person;
    irishRel:genName "Bresul Bricc";
    irishRel:nomName "Bresal Brecc";
    owl:sameAs <http://example.com/LL/rig_ossairge.ttl#BresailBric>;
    owl:sameAs <http://example.com/LL/lagin.ttl#BressalBrecc>;
    rdfs:comment "Ic Bresul Bricc condrecat Lagin & Ossairge.".

<#BresulBelach>
    a foaf:Person;
    irishRel:genName "Bresul Belach";
    irishRel:nomName "Bresal Belach";
    owl:sameAs <http://example.com/LL/dal_corpri_arad#BresalBelachsa>;
    rdfs:comment "Ic Bresul Belach condrecat .h. Fhaelain & .h. Dunchada & h. Muridaig fri Uib Censelaig.".

<#CathairMór>
    a foaf:Person;
    irishRel:nomName "Cathair Mór";
    owl:sameAs <http://example.com/LL/dal_corpri_arad.ttl#CathairMár>;
    rdfs:comment "Ic Cathair Mór condrecat .h. Falgi & h. Enechglais & h. Bairrchi. & h. Crimthainn & Clanda Cathair archena.".

<#Diarmait>
    a foaf:Person;
    irishRel:nomName "Diarmait";
    owl:sameAs <http://example.com/LL/ríg_mide.ttl#Diarmata>;
    rdfs:comment "Ic Diarmait mc Cerbaill condrecat Cland Cholmain & Sil Aeda Slane.".

<#Cerbaill>
    a foaf:Person;
    irishRel:genName "Cerbaill";
    irishRel:nomName "Cerball";
    owl:sameAs <http://example.com/LL/ríg_mide.ttl#FhergusaCerrbeoil>.

<#Flathbertach>
    a foaf:Person;
    irishRel:nomName "Flathbertach";
    owl:sameAs <http://example.com/LL/rig_ceniuil_conaill.ttl#Flaithbertach>;
    rdfs:comment "Ic Flathbertach mc Longsich condrecat Cenel Conaill .i. h. Mael- Doraid. & h. Chanannain.".

<#Longsich>
    a foaf:Person;
    irishRel:genName "Longsich";
    irishRel:nomName "Loingsech";
    owl:sameAs <http://example.com/LL/rig_ceniuil_conaill.ttl#Longsich>.

<#Cund>
    a foaf:Person;
    irishRel:genName "Cund";
    irishRel:nomName "Conn";
    owl:sameAs <http://example.com/LL/rig_ailig.ttl#CuindCetchathaig>;
    rdfs:comment "Ic Cund tra fodlaiter saerchlanna Sil Chuind. Et it fortuatha Síl Chuind cach oen nach berar genelach co Cond eter noebu & rigu. amal atát Lugni Delmna. & Galenga & Cianachta.".

<#Cathair>
    a foaf:Person;
    irishRel:nomName "Cathair";
    owl:sameAs <#CathairMór>;
    rdfs:comment "Ic Cathair dano fodlaiter saerclanda Lagen & it fortuatha cóicid Cathair cach oen na berar genelach cu Cathair amal atát Laigsi & Fotharta.".

# Does Cathair Mór really appear twice? EPT

<#AilillÓlom>
    a foaf:Person;
    irishRel:nomName "Ailill Ólom";
    owl:sameAs <http://example.com/LL/clanna_ébir_i_l-leith_chuind.ttl#AilillAulomm>;
    rdfs:comment "Ic Ailill Ólom fodlaiter saerclanna Muman. & cach oen nad berar genelach co Ailill Ólom. It fortuatha Síl n-Echach Múmo amal atát h-Eraind & Ciarraigi .&c.".

<#Lugaid>
    a foaf:Person;
    irishRel:nomName "Lugaid";
    owl:sameAs <http://example.com/LL/darine_sil_lugdach.ttl#Lugdach>;
    rdfs:comment "O Lugaid mc Itha immorro Corco Laigde & na Callraige.".

<#Ith>
    a foaf:Person;
    irishRel:nomName "Ith";
    owl:sameAs <http://example.com/LL/darine_sil_lugdach.ttl#Itha>.

<#Glass>
    a foaf:Person;
    irishRel:nomName "Glass";
    rel:childOf <#ConaillChostamail>;
    rel:childOf <#Creidne>.

<#Runtar>
    a foaf:Person;
    irishRel:nomName "Runtar";
    rel:childOf <#ConaillChostamail>;
    rel:childOf <#Creidne>.
<#Imda>
    a foaf:Person;
    irishRel:nomName "Imda";
    rel:childOf <#ConaillChostamail>;
    rel:childOf <#Creidne>.

<#ConaillChostamail>
    a foaf:Person;
    irishRel:genName "Conaill Chostamail";
    irishRel:nomName "Conall Costamail";
    rdfs:comment "Ba rí dano cóicid Choncobuir in Conall Costomail.".

# Does this mean he was king of Ulster? EPT

<#Creidne>
    a foaf:Person;
    irishRel:nomName "Creidne";
    foaf:gender "female";
    foaf:title "banfhennid"@sga;
    rel:childOf <#ConaillChostamail>;
    rel:spouseOf <#ConaillChostamail>.

# Does rel:spouseOf cover only marital relations? EPT

<#Aife>
    a foaf:Person;
    irishRel:nomName "Aife";
    foaf:gender "female";
    rel:spouseOf <#ConaillChostamail>;
    rel:antagonistOf <#Creidne>.

<#ConallCostamail>
    a foaf:Person;
    irishRel:nomName "Conall Costamail";
    rel:childOf <#Fhianchada>;
    owl:sameAs <#ConaillChostamail>.

<#Fhianchada>
    a foaf:Person;
    irishRel:genName "Fhianchada";
    irishRel:nomName "Fianchad";
    rel:childOf <#Oengusa>.

<#Oengusa>
    a foaf:Person;
    irishRel:genName "Oengusa";
    irishRel:nomName "Óengus";
    rel:childOf <#Gaith>.

<#Gaith>
    a foaf:Person;
    irishRel:genName "Gaith";
    irishRel:nomName "Gaeth";
    rel:childOf <#Enbláith>.

<#Enbláith>
    a foaf:Person;
    irishRel:genName "Enbláith";
    irishRel:nomName "Enbláth";
    rel:childOf <#Rochada>.

<#Rochada>
    a foaf:Person;
    irishRel:genName "Rochada";
    irishRel:nomName "Rochaid";
    rel:childOf <#IriéilGlunmair>.

<#IriéilGlunmair>
    a foaf:Person;
    irishRel:genName "Iriéil Glunmair";
    irishRel:nomName "Irial Glunmar";
    rel:childOf <#ConaillCernaig>;
    owl:sameAs <http://example.com/LL/dáil_araide.ttl#IrielGlunmair>.

<#ConaillCernaig>
    a foaf:Person;
    irishRel:genName "Conaill Cernaig";
    irishRel:nomName "Conall Cernach";
    rel:childOf <#Amargin>;
    owl:sameAs <http://example.com/LL/clanna_ébir_i_l-leith_chuind.ttl#ConaillCernaich>;
    owl:sameAs <http://example.com/LL/dáil_araide.ttl#ConaillCernaig>.

<#Amargin>
    a foaf:Person;
    irishRel:genName "Amargin";
    irishRel:nomName "Amairgen";
    owl:sameAs <http://example.com/LL/lagin.ttl#Amairgin>.

<#Lebarcham>
    a foaf:Person;
    irishRel:nomName "Lebarcham";
    foaf:gender "female";
    rel:childOf <#Oe>;
    rel:childOf <#Adarce>;
    rel:spouseOf <#ChoinChulainn>.

<#Oe>
    a foaf:Person;
    irishRel:nomName "Oe".

<#Adarce>
    a foaf:Person;
    irishRel:nomName "Adarce".

<#ChoinChulainn>
    a foaf:Person;
    irishRel:genName "Choin Chulainn";
    irishRel:nomName "Cú Chulainn";
    owl:sameAs <http://example.com/LL/dáil_araide.ttl#CuChulaind>.

<#Caulnia>
    a foaf:Person;
    irishRel:nomName "Caulnia";
    rel:childOf <#ChoinChulainn>;
    rel:childOf <#Lebarcham>;
    rdfs:comment "Is dib Corco Caullain & Dal Cualni i Cruithniu.".

<#Condlúan>
    a foaf:Person;
    irishRel:nomName "Condlúan";
    rel:childOf <#ChoinChulainn>;
    rel:childOf <#Lebarcham>;
    rdfs:comment "Is dib Corco Caullain & Dal Cualni i Cruithniu.".
